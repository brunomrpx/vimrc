syntax on
syntax enable
set t_Co=256
set background=dark
colorscheme peachpuff 
set number
set smartindent
set tabstop=4
set shiftwidth=4
set expandtab
set wrap
set linebreak
set laststatus=2
set cursorline
set wildmenu
set hlsearch
set lazyredraw
set showmatch
set incsearch
set title
